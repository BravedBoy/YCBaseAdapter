<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.yc.cn.ycbaseadapter">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".first.FirstActivity"/>
        <activity android:name=".second.SecondActivity"/>
        <activity android:name=".third.ThirdActivity"/>
        <activity android:name=".four.FourActivity"/>
        <activity android:name=".five.FiveActivity"/>
    </application>

</manifest>